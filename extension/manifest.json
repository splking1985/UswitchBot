{
	"manifest_version": 2,
	"name": "FormFiller",
	"version": "1.0.0",
	"description": "A Form Filler",
	"background": {
		"scripts": ["/jquery-2.2.4.min.js", "/background.js"]
	},
	"browser_action": {
		"browser_style": false,
		"default_icon": {
			"128": "128.png"
		},
		"default_title": "Form Filler",
		"default_popup": "/index.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
	"permissions": [
		"tabs",
		"*://*/*"
	]
}